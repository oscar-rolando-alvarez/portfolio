{
  "name": "defi-lending-protocol",
  "version": "1.0.0",
  "description": "A complete DeFi lending protocol on Solana",
  "main": "index.js",
  "scripts": {
    "lint:fix": "prettier --write '{**/*,*}.{js,ts,jsx,tsx}'",
    "lint": "prettier --check '{**/*,*}.{js,ts,jsx,tsx}'",
    "test": "anchor test",
    "test:unit": "jest",
    "build": "anchor build",
    "start-validator": "solana-test-validator",
    "deploy:localnet": "anchor deploy --provider.cluster localnet",
    "deploy:devnet": "anchor deploy --provider.cluster devnet",
    "app:dev": "cd app && npm run dev",
    "app:build": "cd app && npm run build"
  },
  "keywords": [
    "solana",
    "defi",
    "lending",
    "anchor",
    "blockchain"
  ],
  "author": "DeFi Protocol Team",
  "license": "MIT",
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@solana/web3.js": "^1.87.6",
    "@solana/spl-token": "^0.4.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.0",
    "jest": "^29.7.0",
    "mocha": "^9.0.0",
    "prettier": "^2.6.2",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.3.5"
  }
}