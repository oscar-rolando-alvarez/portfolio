{
  "name": "vue-project-management",
  "version": "1.0.0",
  "private": true,
  "description": "Comprehensive project management application built with Vue 3, Nuxt 3, and TypeScript",
  "keywords": ["vue3", "nuxt3", "typescript", "project-management", "kanban", "gantt", "pwa"],
  "author": "Vue Project Management Team",
  "license": "MIT",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "docker:build": "docker build -t vue-project-management .",
    "docker:run": "docker run -p 3000:3000 vue-project-management"
  },
  "dependencies": {
    "@nuxt/ui": "^2.18.7",
    "@pinia/nuxt": "^0.5.5",
    "@vueuse/core": "^11.2.0",
    "@vueuse/nuxt": "^11.2.0",
    "gantt-task-react": "^0.3.9",
    "nuxt": "^3.14.159",
    "pinia": "^2.2.6",
    "vue": "^3.5.13",
    "vue-router": "^4.4.5",
    "vue3-drag-drop": "^1.0.4",
    "vuedraggable": "^4.1.0",
    "@vueup/vue-quill": "^1.2.0",
    "chart.js": "^4.4.6",
    "vue-chartjs": "^5.3.1",
    "date-fns": "^4.1.0",
    "socket.io-client": "^4.8.1",
    "jwt-decode": "^4.0.0",
    "axios": "^1.7.9",
    "lucide-vue-next": "^0.468.0",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "v-calendar": "^3.1.2",
    "vue-toastification": "^2.0.0-rc.5",
    "file-saver": "^2.0.5",
    "jspdf": "^2.5.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.6.1",
    "@nuxt/eslint-config": "^0.7.3",
    "@nuxt/test-utils": "^3.15.3",
    "@nuxt/typescript-build": "^3.0.2",
    "@pinia/testing": "^0.1.6",
    "@types/file-saver": "^2.0.7",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.31.0",
    "happy-dom": "^15.11.6",
    "prettier": "^3.4.2",
    "sass": "^1.82.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "vue-tsc": "^2.1.10",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "tailwindcss": "^3.4.17",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}